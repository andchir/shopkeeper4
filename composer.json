{
    "name": "andchir/shopkeeper4",
    "type": "project",
    "license": "proprietary",
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "alcaeus/mongo-php-adapter": "^1.1",
        "andchir/omnipay-bundle": "^1.0.17",
        "andchir/shopkeeper4-comments": "^1.0",
        "andchir/twig-visual": "^1.0",
        "andrewnovikof/omnipay-sberbank": "^3.2",
        "behat/transliterator": "^1.3",
        "composer/composer": "^1.8",
        "doctrine/cache": "^1.8",
        "doctrine/mongodb-odm-bundle": "^4.0",
        "gregwar/captcha-bundle": "^2.0",
        "laminas/laminas-code": "^3.4",
        "league/omnipay": "^3.0",
        "liip/imagine-bundle": "^2.3",
        "mongodb/mongodb": "^1.4",
        "phpoffice/phpspreadsheet": "^1.10",
        "ralouphie/mimey": "^2.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "5.0.*",
        "symfony/cache": "5.0.*",
        "symfony/config": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/process": "5.0.*",
        "symfony/requirements-checker": "^1.1",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "5.0.*",
        "symfony/twig-bundle": "5.0.*",
        "symfony/validator": "5.0.*",
        "symfony/web-link": "5.0.*",
        "symfony/yaml": "5.0.*",
        "doctrine/doctrine-fixtures-bundle": "^3.3"
    },
    "require-dev": {
        "symfony/browser-kit": "5.0.*",
        "symfony/css-selector": "5.0.*",
        "symfony/debug-bundle": "5.0.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/stopwatch": "5.0.*",
        "symfony/test-pack": "^1.0",
        "symfony/web-profiler-bundle": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "ext-mongo": "1.6.16",
            "php": "7.3.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Andchir\\ImportExportBundle\\": "vendor/andchir/import-export-bundle/",
            "Andchir\\DigitalGoodsBundle\\": "vendor/andchir/digital-goods-bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*",
            "id": "01DVARZ9TKCSPXMS89SPV7FNJA"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
